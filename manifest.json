{
    "manifest_version": 3,
    "name": "Tab Title Fetcher",
    "version": "1.0",
    "description": "Fetches the title of the current tab",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  